datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

enum Sex {
  MALE
  FEMALE
  OTHER
}

model User {
  id        String   @id @default(uuid())
  email     String   @unique
  name      String?
  age       Int?
  sex       Sex?
  contact   String?
  student   Boolean?
  createdAt DateTime @default(now())
}
